    <validationRules>
        <fullName>ExpiresAfterUsagePositiveInteger</fullName>
        <active>true</active>
        <errorConditionFormula>if( Expires_after_usage__c &lt;= 0,true,false)</errorConditionFormula>
        <errorDisplayField>Expires_after_usage__c</errorDisplayField>
        <errorMessage>You must enter a value greater than zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IfReferrerPayment1IsNotNullRequireOther</fullName>
        <active>true</active>
        <errorConditionFormula>IF( 
AND( 
NULLVALUE(Referrer_Payment_Amount1__c,0)&gt;0, 
TEXT(At_Stage1__c)=&apos;&apos;, 
TEXT(Referral_Payout_on1__c)=&apos;&apos;, 
TEXT(Referrer_Payment_Option1__c)=&apos;&apos;) 
,true,false)</errorConditionFormula>
        <errorDisplayField>Referral_Payout_on1__c</errorDisplayField>
        <errorMessage>If you enter a Referrer Amount, you also need to enter values for Payout On, At Stage, and Payment Option.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IfReferrerPayment2IsNotNullRequireOther</fullName>
        <active>true</active>
        <errorConditionFormula>IF( 
AND( 
NULLVALUE(Referrer_Payment_Amount2__c,0)&gt;0, 
TEXT(At_Stage2__c)=&apos;&apos;, 
TEXT(Referral_Payout_on2__c)=&apos;&apos;, 
TEXT(Referrer_Payment_Option2__c)=&apos;&apos;)
,true,false)</errorConditionFormula>
        <errorDisplayField>Referral_Payout_on2__c</errorDisplayField>
        <errorMessage>If you enter a Referrer Amount, you also need to enter values for Payout On, At Stage, and Payment Option.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IfReferrerPayment3IsNotNullRequireOther</fullName>
        <active>true</active>
        <errorConditionFormula>IF( 
AND( 
NULLVALUE(Referrer_Payment_Amount3__c,0)&gt;0, 
TEXT(At_Stage3__c)=&apos;&apos;, 
TEXT(Referral_Payout_on3__c)=&apos;&apos;, 
TEXT(Referrer_Payment_Option3__c)=&apos;&apos;) 
,true,false)</errorConditionFormula>
        <errorDisplayField>Referral_Payout_on3__c</errorDisplayField>
        <errorMessage>If you enter a Referrer Amount, you also need to enter values for Payout On, At Stage, and Payment Option.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>InTimeDurationLengthPositiveValue</fullName>
        <active>true</active>
        <errorConditionFormula>IF( In_Time_Duration_Length__c &lt;= 0,true,false)</errorConditionFormula>
        <errorDisplayField>In_Time_Duration_Length__c</errorDisplayField>
        <errorMessage>You must enter an In Time Duration Length greater than zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MustEnterAtLeastOneRule</fullName>
        <active>false</active>
        <errorConditionFormula>If( AND(
NULLVALUE(Referrer_Payment_Amount1__c,0)=0,
NULLVALUE(Referrer_Payment_Amount2__c,0)=0,
NULLVALUE(Referrer_Payment_Amount3__c,0)=0),true,false)</errorConditionFormula>
        <errorMessage>You must enter at least one amount for a referrer payment</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RefereePayment1GreaterThanOrEqualToZero</fullName>
        <active>true</active>
        <errorConditionFormula>If(Referee_Payment_Amount1__c &lt; 0, true, false)</errorConditionFormula>
        <errorDisplayField>Referee_Payment_Amount1__c</errorDisplayField>
        <errorMessage>Payment Amount must be greater than zero</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RefereePayment2GreaterThanZero</fullName>
        <active>true</active>
        <errorConditionFormula>IF( Referee_Payment_Amount2__c &lt; 0, true, false)</errorConditionFormula>
        <errorDisplayField>Referee_Payment_Amount1__c</errorDisplayField>
        <errorMessage>Payment amount must be greater than zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RefereePayment3GreaterThanZero</fullName>
        <active>true</active>
        <errorConditionFormula>IF( Referee_Payment_Amount3__c &lt; 0, true,false)</errorConditionFormula>
        <errorDisplayField>Referee_Payment_Amount3__c</errorDisplayField>
        <errorMessage>Payment amount must be greater than zero</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ReferrerPayment1GreaterThanZero</fullName>
        <active>false</active>
        <errorConditionFormula>IF( Referrer_Payment_Amount1__c &lt; 0, true, false)</errorConditionFormula>
        <errorDisplayField>Referrer_Payment_Amount1__c</errorDisplayField>
        <errorMessage>Payment amount must be greater than zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ReferrerPayment2GreaterThanZero</fullName>
        <active>true</active>
        <errorConditionFormula>if( Referrer_Payment_Amount2__c&lt;0 ,true,false)</errorConditionFormula>
        <errorDisplayField>Referrer_Payment_Amount2__c</errorDisplayField>
        <errorMessage>Payment amount must be greater than zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ReferrerPayment3GreaterThanZero</fullName>
        <active>true</active>
        <errorConditionFormula>IF( Referrer_Payment_Amount3__c &lt;0,true,false)</errorConditionFormula>
        <errorDisplayField>Referrer_Payment_Amount3__c</errorDisplayField>
        <errorMessage>Payment amount must be greater than zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RuleCriteriaNeedsAllThree</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
AND(NOT(ISNULL(Expires_after_usage__c )),
OR(ISNULL(In_Time_Duration_Length__c) ,ISPICKVAL(In_Time_Duration_Unit__c,&apos;&apos;))),true,
IF(
AND(NOT(ISNULL(In_Time_Duration_Length__c)),
OR(ISNULL(Expires_after_usage__c ) ,ISPICKVAL(In_Time_Duration_Unit__c,&apos;&apos;))),true,
IF(
AND(NOT(ISPICKVAL(In_Time_Duration_Unit__c,&apos;&apos;)),
OR(ISNULL(In_Time_Duration_Length__c) ,ISNULL(Expires_after_usage__c ))),true,
false)))</errorConditionFormula>
        <errorMessage>If you enter a value for one of the following: Expires after # usage, In Time Duration Length, or In Time Duration Unit, then all three must have a value entered.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_date_should_be_lesser_than_enddate</fullName>
        <active>true</active>
        <errorConditionFormula>Start_Date__c &gt; End_Date__c</errorConditionFormula>
        <errorMessage>Start Date should be less than or equal to the End Date</errorMessage>
    </validationRules>
