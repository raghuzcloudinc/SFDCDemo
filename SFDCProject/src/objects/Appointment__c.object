    </validationRules>
    <validationRules>
        <fullName>Appointment_Cancellation_Reason_Required</fullName>
        <active>false</active>
        <errorConditionFormula>AND( 
ISPICKVAL(Status__c,&quot;Appointment Cancelled&quot;), 
ISBLANK( Text(Cancellation_Reason__c)) 
)</errorConditionFormula>
        <errorDisplayField>Cancellation_Reason__c</errorDisplayField>
        <errorMessage>Please fill out Cancellation Reason</errorMessage>
