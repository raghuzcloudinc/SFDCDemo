        <externalId>false</externalId>
        <formula>IF (ISPICKVAL(Customer_Signed__c, &quot;Wet Signed&quot;), &quot;Wet Signed&quot;, 
IF (AND (ISPICKVAL(Customer_Signed__c, &quot;No&quot;),ISPICKVAL( DocuSign_Status__c , &quot;Completed&quot;)), &quot;e-signatures completed&quot; , 
IF (AND (ISPICKVAL(Customer_Signed__c, &quot;No&quot;),ISPICKVAL( DocuSign_Status__c , &quot;Voided&quot;)), &quot;e-signatures voided&quot; , 
IF (AND (ISPICKVAL(Customer_Signed__c, &quot;No&quot;),ISPICKVAL( DocuSign_Status__c , &quot;Declined&quot;)), &quot;e-signatures declined&quot;, &quot;e-signatures in progress&quot;) )))</formula>
