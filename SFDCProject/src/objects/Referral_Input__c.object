        <errorConditionFormula>AND
(
ISCHANGED(   Promotion__c  ),
OR(
TEXT(Milestone_1_Status__c) != &apos;&apos;,
TEXT(Milestone_2_Status__c) != &apos;&apos;, 
TEXT( Milestone_3_Status__c) != &apos;&apos;),
$User.ProfileId != &quot;00e60000000nGtn&quot;,
$User.ProfileId != &quot;00e60000001BXgk&quot;
)</errorConditionFormula>
        <errorDisplayField>Promotion__c</errorDisplayField>
        <errorMessage>Promotion can not be changed if referral is / has passed &apos;Qualified for Payment&apos; stage.</errorMessage>
