        <fullName>Submit_Proposal</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Submit To Sunrun</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/20.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/20.0/apex.js&quot;)} 

try{ 

/* 
var isSubmitted ={!IF( AND(ISPICKVAL(Proposal__c.Stage__c,&apos;Created&apos;), (Proposal__c.Sales_Partner__c = &apos;Sunrun&apos;),ISPICKVAL(Proposal__c.Agreement_Type__c,&apos;Customer Owned - Full Upfront&apos;), NOT(OR(ISPICKVAL(Proposal__c.Finance_Type__c,&apos;N/A&apos;),ISPICKVAL(Proposal__c.Finance_Type__c, &apos;Bank Financed&apos;)))), &apos;true&apos; , &apos;false&apos;)} 
*/ 

var showModalWindow = {!IF( 
OR(ISPICKVAL(Proposal__c.Stage__c,&apos;Created&apos;), 
ISPICKVAL(Proposal__c.Stage__c,&apos;Ready for Submission&apos;)) 
, &apos;true&apos;, &apos;false&apos;)}; 

if(showModalWindow == false){ 
var isApproved = {!IF( OR(ISPICKVAL(Proposal__c.Stage__c,&apos;SR Ops Approved&apos;)) , &apos;true&apos;, &apos;false&apos;)}; 
if(isApproved == true){ 
alert(&apos;This is an approved proposal. You can not resubmit it again&apos;); 
} 

else{ 
alert(&apos;Invalid proposal stage for submission&apos;); 
} 

} 
/* 
else if(isSubmitted == true){ 

alert(&apos;Since this is a proposal for Customer Owned system, please specify Finance Type (either Bank Financed or N/A)&apos;); 
} 
*/ 
else{ 

var isCOBFdetailFilled = {!IF(
(AND (
ISPICKVAL(Proposal__c.Agreement_Type__c, &apos;Customer Owned - Bank Financed&apos;),
OR(ISBLANK(Proposal__c.Shopping_Pass__c),
ISBLANK(Proposal__c.CVV__c),ISBLANK(Proposal__c.Max_Loan_Amount__c),
ISBLANK(Proposal__c.Dealer_Fee__c),
ISBLANK (Proposal__c.ExpirationDate__c),
ISBLANK(TEXT(Proposal__c.Shopping_Pass_Expiration_Month__c)), 
ISBLANK(TEXT(Proposal__c.Shopping_Pass_Expiration_Year__c))
 )

)), &apos;true&apos;, &apos;false&apos; )};

if( isCOBFdetailFilled == true){
alert(&apos;Customer Owned Bank Financed proposal needs Max Loan Amount, Dealer Fee,Shopping Pass,Shopping Pass Expiration Year, Shopping Pass Expiration Month, Loan Expiration Date and CVV number for Submission&apos;);
}
else { 
var opportunityId = &apos;{!Proposal__c.OpportunityId__c}&apos;; 
var isCancel = false; 
var responseValue = sforce.apex.execute(&quot;EDPWebServices&quot;,&quot;canUserSubmitProposal&quot;, {objectId:opportunityId}); 
if(responseValue != null){ 
if(responseValue[0].status != null){ 
if(responseValue[0].status == &apos;Warning&apos; || responseValue[0].status == &apos;SUCCESS&apos;){ 
isCancel = window.confirm(responseValue[0].errorMessage); 
}else if(responseValue[0].status == &apos;Error&apos;){ 
alert(responseValue[0].errorMessage); 
} 
} 
}else{ 
isCancel = window.confirm(&apos;Are you sure you would like to submit this proposal?&apos;); 
} 

if(isCancel == true){ 
var proposalIdVal = &apos;{!Proposal__c.Id}&apos; ; 
var proposalSage = &apos;{!Proposal__c.Stage__c}&apos; ; 
var result = sforce.apex.execute(&quot;EDPWebServices&quot;,&quot;submitProposal&quot;, 
{proposalId:proposalIdVal}); 
alert(result); 
location.reload(true); 
}
} 
} 
}catch(exceptionObj){ 
if(exceptionObj.faultstring != null){ 
var faultStr = exceptionObj.faultstring; 
var n = faultStr.indexOf(&quot;no access allowed to this class&quot;); 
if( n &gt;= 0){ 
alert(&apos;You don\&apos;t have sufficient access to invoke this service&apos;); 
}else{ 
alert(exceptionObj.faultstring); 
} 
}else{ 
alert(exceptionObj); 
} 
}</url>
