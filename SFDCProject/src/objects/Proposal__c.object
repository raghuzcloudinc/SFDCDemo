    <validationRules>
        <fullName>Block_SR_Ops_Actions_to_On_Hold</fullName>
        <active>true</active>
        <description>Block SR Ops Actions to On Hold if Stage is (Expired, Inactive, Replaced, Voided, Withdrawn)</description>
        <errorConditionFormula>AND(OR(ISPICKVAL(Stage__c,&quot;Expired&quot;), 
ISPICKVAL(Stage__c, &quot;Inactive&quot;),
ISPICKVAL(Stage__c, &quot;Replaced&quot;),
ISPICKVAL(Stage__c, &quot;Voided&quot;),
ISPICKVAL(Stage__c, &quot;Withdrawn&quot;)),ISPICKVAL(SR_Ops_Actions__c,&apos;On Hold&apos;))</errorConditionFormula>
        <errorMessage>SR Ops Actions can not be modified if the Stage is either Expired, Inactive, Replaced,Voided or Withdrawn</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>COBF_Data_Validations</fullName>
        <active>true</active>
        <description>Customer Owned Bank Financed proposal needs Max Loan Amount and Dealer Fee specified before SR Approval</description>
        <errorConditionFormula>AND (
ISPICKVAL(Agreement_Type__c, &apos;Customer Owned - Bank Financed&apos;), 
ISPICKVAL(Stage__c, &apos;SR Approved&apos;),
OR(
ISBLANK(Max_Loan_Amount__c),
ISBLANK(Dealer_Fee__c)
)
)</errorConditionFormula>
        <errorMessage>Customer Owned Bank Financed proposal needs Max Loan Amount and Dealer Fee specified before SR Approval</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cant_update_oppty_associated_w_proposal</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISCHANGED(Opportunity__c), 
$Profile.Name &lt;&gt; &apos;System Administrator&apos;, 
$Profile.Name &lt;&gt; &apos;Operations&apos;, 
$User.Id &lt;&gt; &apos;00560000001PYJnAAO&apos;, 
$User.Id &lt;&gt; &apos;005600000046gQ4&apos;, 
$User.Id &lt;&gt; &apos;00560000003KtpG&apos;, 
$User.Id &lt;&gt; &apos;00560000001Pfen&apos;, 
$User.Id &lt;&gt; &apos;00560000002G4QB&apos;, 
$User.Id &lt;&gt; &apos;005600000046gQE&apos;,
$User.Id &lt;&gt; &apos;00560000001P2RL&apos;
)</errorConditionFormula>
        <errorDisplayField>X1st_Customer_Payment_Terms__c</errorDisplayField>
        <errorMessage>Opportunity associated with proposal can not be edited.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_For_Declined_Reason</fullName>
        <active>true</active>
        <description>When the Stage equals SR Declined then Declined Reason must be selected</description>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;System Administrator&quot; 
,(ISPICKVAL(Stage__c,&quot;SR Declined&quot;))
,ISBLANK(Declined_Proposal_Reason__c) 
)</errorConditionFormula>
        <errorDisplayField>Declined_Proposal_Reason__c</errorDisplayField>
        <errorMessage>Select Declined Reason(s)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_for_Dealer_Fee_amount</fullName>
        <active>true</active>
        <description>Dealer Fee should be no more than 20.00%</description>
        <errorConditionFormula>Dealer_Fee__c &gt; 0.20</errorConditionFormula>
        <errorMessage>Dealer Fee cannot exceed 20.00%</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Costco_Proposal_Creation_By_PT</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISNEW() ,
    ISPICKVAL(Proposal_Source__c , &apos;PT&apos;) ,
    $User.Id = &apos;005600000010Nel&apos; ,
    ISPICKVAL(Opportunity__r.Opportunity_Source_Type__c,     
    &apos;BB&apos;) , 
 OR(
 CONTAINS( TEXT(Opportunity__r.Lead_Source_2__c),&quot;Costco&quot;), 
 ISPICKVAL( Opportunity__r.Purchased_Thru__c , &quot;Costco&quot;) 
   ) 
)</errorConditionFormula>
        <errorMessage>A lead has already been created for this customer.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Credit_Vs_Ops_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
AND(
$Profile.Name &lt;&gt; &quot;Finance - Credit&quot;
,$Profile.Name &lt;&gt; &quot;System Administrator&quot;
,ISCHANGED(Pending_Proposal_Reason__c)
,OR(
AND(NOT(INCLUDES(PRIORVALUE(Pending_Proposal_Reason__c),&quot;Missing sig. of homeowner who pass cr&quot;))
,INCLUDES(Pending_Proposal_Reason__c,&quot;Missing sig. of homeowner who pass cr&quot;))
,
AND(NOT(INCLUDES(PRIORVALUE(Pending_Proposal_Reason__c),&quot;Minor change order&quot;))
,INCLUDES(Pending_Proposal_Reason__c,&quot;Minor change order&quot;))
,
AND(NOT(INCLUDES(PRIORVALUE(Pending_Proposal_Reason__c),&quot;Major change order&quot;))
,INCLUDES(Pending_Proposal_Reason__c,&quot;Major change order&quot;))
,
AND(NOT(INCLUDES(PRIORVALUE(Pending_Proposal_Reason__c),&quot;Need Cond. Approval or Full Payment&quot;))
,INCLUDES(Pending_Proposal_Reason__c,&quot;Need Cond. Approval or Full Payment&quot;))
,
AND(NOT(INCLUDES(PRIORVALUE(Pending_Proposal_Reason__c),&quot;Need Credit Check:&quot;))
,INCLUDES(Pending_Proposal_Reason__c,&quot;Need Credit Check:&quot;))
,
AND(NOT(INCLUDES(PRIORVALUE(Pending_Proposal_Reason__c),&quot;Need Mortgage Statement&quot;))
,INCLUDES(Pending_Proposal_Reason__c,&quot;Need Mortgage Statement&quot;))
,
