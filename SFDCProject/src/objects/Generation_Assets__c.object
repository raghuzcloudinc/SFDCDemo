    <validationRules>
        <fullName>PTO_and_Meter_Serial_Num_and_Meter_Reg</fullName>
        <active>false</active>
        <description>PTO Date cannot be specified if Meter Serial Num and Meter Registration Date are not also specified. Refer to BSKY-692 part 2 &amp; BSKY-1083</description>
        <errorConditionFormula>AND(
	NOT(
		OR(
			ISPICKVAL( Asset_Status__c,&apos;Removed - Permanent&apos;),
			ISPICKVAL( Asset_Status__c,&apos;Deactivated&apos;),
			ISPICKVAL( Asset_Status__c,&apos;Redeployed&apos;)
			)
		), 
	(ISBLANK (Meter_Serial_Num__c ) || ISBLANK ( Meter_Test_Date__c) ) &amp;&amp; NOT (ISBLANK ( PTO__c ))
	)</errorConditionFormula>
        <errorMessage>PTO Date cannot be specified if Meter Serial Num and Meter Registration Date are not also specified. Likewise, Meter Serial Num and Meter Registration Date are required if PTO Date is populated.</errorMessage>
    </validationRules>
    <validationRules>
