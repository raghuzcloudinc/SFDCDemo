        <formula>CASE
(1, IF(and((CONTAINS( Installer__r.Name ,&quot;REC Solar&quot;)),(ISPICKVAL( Initial_Deposit_Method__c ,&quot;Paid to Partner&quot;))),1,0),
(EPC_Price__c-Initial_Deposit_Amount__c)*M1_terms__c,
IF(ISPICKVAL(Install_Partner__c,&quot;Mercury Solar&quot;),1,0),(EPC_Price__c- Initial_Deposit_Amount__c )* M1_terms__c ,
IF(CONTAINS(Installer__r.Name,&quot;American Solar&quot;),1,0),M1_terms__c ,
IF(and((CONTAINS( Installer__r.Name ,&quot;UR Solar&quot;)), Verengo_Financed__c = TRUE),1,0), EPC_Price__c * 0,
IF(and((CONTAINS( Installer__r.Name ,&quot;UR Solar&quot;)),(ISPICKVAL( Initial_Deposit_Method__c ,&quot;Paid to Partner&quot;))),1,0),
EPC_Price__c* M1_terms__c -Initial_Deposit_Amount__c,
IF(and((CONTAINS( Installer__r.Name ,&quot;UR Solar&quot;)),(ISPICKVAL( Initial_Deposit_Method__c ,&quot;Paid to Partner&quot;)), Verengo_Financed__c = TRUE),1,0), EPC_Price__c* 0,
IF(and((CONTAINS( Installer__r.Name ,&quot;American Electric&quot;)), Verengo_Financed__c = TRUE),1,0), EPC_Price__c * 0,
IF(and((CONTAINS( Installer__r.Name ,&quot;American Electric&quot;)),(ISPICKVAL( Initial_Deposit_Method__c ,&quot;Paid to Partner&quot;))),1,0),
EPC_Price__c* M1_terms__c -Initial_Deposit_Amount__c,
IF(and((CONTAINS( Installer__r.Name ,&quot;American Electric&quot;)),(ISPICKVAL( Initial_Deposit_Method__c ,&quot;Paid to Partner&quot;)), Verengo_Financed__c = TRUE),1,0), EPC_Price__c * 0,
IF(and((CONTAINS( Installer__r.Name ,&quot;Trinity Solar&quot;)),(ISPICKVAL( Initial_Deposit_Method__c ,&quot;Paid to Partner&quot;))),1,0),
(EPC_Price__c-Initial_Deposit_Amount__c)*M1_terms__c,
EPC_Price__c* M1_terms__c)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
