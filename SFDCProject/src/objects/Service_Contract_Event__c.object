)</errorConditionFormula>
        <errorMessage>Both Rate Modification and Rate Modification Date must be populated together.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Rebate_Conf_Num_needed_for_NTP_Granted</fullName>
        <active>true</active>
        <description>Ref: BSKY-2834 
NTP Granted field can not be specified if Rebate Conf Num field is blank.</description>
        <errorConditionFormula>AND 
( 
OR(Service_Contract__r.State__c = &apos;CO&apos;, Service_Contract__r.State__c = &apos;CT&apos;), 
ISBLANK(Rebate_Conf_Num__c), 
NOT(ISBLANK(NTP_Granted__c)) 
)</errorConditionFormula>
        <errorDisplayField>Adjustment_to_Credit_Memo_Amt__c</errorDisplayField>
        <errorMessage>For asset in CO or CT, NTP Granted field can not be specified if Rebate Conf Num field is blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_on_Credit_Memo_on_Rebate</fullName>
        <active>true</active>
        <description>Implemented for BSKY-6470</description>
        <errorConditionFormula>AND
(
