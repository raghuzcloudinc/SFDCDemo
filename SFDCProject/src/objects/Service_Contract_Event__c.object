    <validationRules>
        <fullName>Lock_Sunrunsouth_SCE</fullName>
        <active>false</active>
        <errorConditionFormula>AND($Profile.Name !=&apos;System Administrator&apos;,$Profile.Name !=&apos;Marketo&apos;, External_Source__c ==&apos;SUNRUN SOUTH&apos;)</errorConditionFormula>
        <errorMessage>Cannot edit sunrun south records</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>M1_requires_NTP</fullName>
        <active>true</active>
        <description>M1 cannot be entered if plans have not been approved and NTP has not been granted.</description>
        <errorConditionFormula>AND
(
OR(ISBLANK(Facility_Design_Approved__c), ISBLANK(NTP_Granted__c)), 
M1_proof_panel_inverter_delivery__c &gt; datevalue(&quot;2011-08-23&quot;),
Service_Contract__r.Install_Partner__c != &quot;Trinity Solar&quot;, 
Service_Contract__r.Install_Partner__c != &quot;American Solar&quot;, 
Service_Contract__r.Install_Partner__c != &quot;American Solar Electric&quot;, 
Service_Contract__r.Install_Partner__c != &quot;American Solar - New Homes&quot; 
)</errorConditionFormula>
        <errorDisplayField>Adjustment_to_Credit_Memo_Amt__c</errorDisplayField>
        <errorMessage>M1 cannot be entered unless plans and NTP have been approved.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>M2_requires_NTP</fullName>
        <active>true</active>
        <description>M2 cannot be entered if plans have not been approved and NTP has not been granted</description>
        <errorConditionFormula>AND 
( 
OR(ISBLANK(Facility_Design_Approved__c), ISBLANK(NTP_Granted__c)), 
M2_proof_substantial_completion__c &gt; datevalue(&quot;2011-08-23&quot;), 
Service_Contract__r.Install_Partner__c != &quot;Trinity Solar&quot;, 
Service_Contract__r.Install_Partner__c != &quot;American Solar&quot;, 
Service_Contract__r.Install_Partner__c != &quot;American Solar Electric&quot; 
)</errorConditionFormula>
        <errorDisplayField>Adjustment_to_Credit_Memo_Amt__c</errorDisplayField>
        <errorMessage>M2 cannot be entered unless plans and NTP have been approved</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NTP_HOA_Validation_v2</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
	OR( 
	  ISPICKVAL(Service_Contract__r.Home_Type__c, &apos;Vertically-split duplex&apos;), 
	  ISPICKVAL(Service_Contract__r.Home_Type__c, &apos;Townhome / rowhome&apos;) , 

	  ISPICKVAL(Service_Contract__r.Home_Type__c, &apos;Single-family detached home in condo development&apos;) 
        ), 

	NOT(ISBLANK(NTP_Granted__c )),
	
	ISBLANK(Attached_Home_Approved__c),

       OR( 
	  $Profile.Name = &apos;Operations&apos;, 
          $Profile.Name = &apos;Operations - Project Coordinator&apos;,
          $Profile.Name = &apos;Operations - Project Coordinator Brea&apos;
        )

	
)</errorConditionFormula>
        <errorDisplayField>Adjustment_to_Credit_Memo_Amt__c</errorDisplayField>
        <errorMessage>As this is an attached home, the multi-family addendum must be signed before NTP is granted. Once approval documents are received, fill in the &apos;Attached Home Approved&apos; date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NTP_validation_rule_1</fullName>
        <active>true</active>
        <description>NTP granted date can not be populated if SR Signoff Date is not specified OR Facility Designed Approved Date is not specified OR Complete Deal Received Date is not specified</description>
        <errorConditionFormula>NOT (ISBLANK( NTP_Granted__c ) || (NTP_Granted__c &lt;= DATE(2012,11,14)) ) 
&amp;&amp; 
( 
ISBLANK(SR_Signoff__c) 
|| 
ISBLANK( Facility_Design_Approved__c ) 
|| 
ISBLANK( Complete_Deal_Received__c) 
)</errorConditionFormula>
        <errorDisplayField>Adjustment_to_Credit_Memo_Amt__c</errorDisplayField>
        <errorMessage>NTP granted date can not be populated if SR Signoff Date is not specified OR Facility Designed Approved Date is not specified OR Complete Deal Received Date is not specified</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NTP_validation_rule_2</fullName>
        <active>false</active>
        <errorConditionFormula>( 
NOT (ISBLANK( NTP_Granted__c ) || (NTP_Granted__c &lt;= DATE(2012,11,14)) ) 

&amp;&amp; 

NOT ( 

( 

( 
NOT (ISNULL(Initial_Deposit_Processed__c)) 
&amp;&amp; 
(Initial_Deposit_Amount_Received__c = Initial_Deposit_Amount__c ) 
) 

|| 

ISPICKVAL (Initial_Deposit_Method__c, &quot;Paid to Partner&quot;) 

) 

|| 

Initial_Deposit_Amount__c = 0 

|| 

(Service_Contract__r.Partner_Financed__c = TRUE &amp;&amp; Service_Contract__r.Total_Solar_Prepay_Required__c = TRUE) 


) 

) 

&amp;&amp; 

Service_Contract__r.Manual_Override__c = False &amp;&amp; $User.Id &lt;&gt; &apos;0056000000453XT&apos;</errorConditionFormula>
        <errorDisplayField>Adjustment_to_Credit_Memo_Amt__c</errorDisplayField>
        <errorMessage>(Initial Deposit Processed must be populated &amp; Initial Deposit Amount Received must be = Initial Deposit Amount) OR Initial Deposit Method = Paid to Partner EXCEPT if Initial Deposit Amount = 0 OR project is a &quot;Hero&quot; project. CAN NOT SPECIFY NTP GRANTED.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NTP_validation_rule_3</fullName>
        <active>false</active>
        <description>NTP Granted Date should not be specified if Large System NTP Form is not on file and System Size DC is greater than or equal to 15kW.</description>
        <errorConditionFormula>NOT (ISBLANK( NTP_Granted__c ) || (NTP_Granted__c &lt;= DATE(2012,11,14)) ) 
&amp;&amp; 
( 
Service_Contract__r.Large_system_NTP_form__c = FALSE 
&amp;&amp; 
Service_Contract__r.System_Size_DC__c  &gt;= 15 
)</errorConditionFormula>
        <errorDisplayField>Adjustment_to_Credit_Memo_Amt__c</errorDisplayField>
        <errorMessage>NTP Granted Date should not be specified if Large System NTP Form is not on file and System Size DC is greater than or equal to 15kW.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NTP_validation_rule_4</fullName>
        <active>false</active>
        <description>Rebate Conf Rec&apos;d and Final Rebate Amount must be filled in OR Credit Memo on Rebate must have a value EXCEPT IF State is HI or it is a new home or install partner is Verengo or Solar Universe or Sunrun or Est Rebate Amount is zero</description>
        <errorConditionFormula>NOT (ISBLANK( NTP_Granted__c ) || (NTP_Granted__c &lt;= DATE(2012,11,14)) ) 
&amp;&amp; 
NOT 
( 
( 
NOT (ISNULL (Rebate_Conf_Rec_d__c) || (ISNULL (Final_Rebate_Amount__c) || Final_Rebate_Amount__c = 0)) 
|| 
(Credit_Memo_on_Rebate__c != 0 &amp;&amp; NOT (ISNULL(Credit_Memo_on_Rebate__c))) 
) 
|| 
Service_Contract__r.State__c = &quot;HI&quot; 
|| 
Est_Rebate_Amount__c = 0 
|| 
Service_Contract__r.Channel__c = &apos;New Homes&apos; 
|| 
Service_Contract__r.Install_Partner__c = &apos;Verengo&apos; 
|| 
Service_Contract__r.Install_Partner__c = &apos;Solar Universe&apos;
||
Service_Contract__r.Install_Partner__c = &apos;Sunrun&apos; 
)</errorConditionFormula>
        <errorDisplayField>Adjustment_to_Credit_Memo_Amt__c</errorDisplayField>
        <errorMessage>Rebate Conf Rec&apos;d and Final Rebate Amount must be filled in OR Credit Memo on Rebate must have a value EXCEPT IF State is HI or it is a new home or install partner is Verengo or Solar Universe or Sunrun or Est Rebate Amount is zero</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_revised_SR_signoff_if_no_SR_signoff</fullName>
        <active>true</active>
        <description>Change order cannot be entered if initial deal was not approved</description>
        <errorConditionFormula>AND
(
ISBLANK(SR_Signoff__c),
NOT(ISBLANK(Revised_SR_Signoff__c))
)</errorConditionFormula>
        <errorMessage>The original deal was not approved for this customer1</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Outstanding_Change_order</fullName>
        <active>true</active>
        <description>There is an outstanding change order for this customer.</description>
        <errorConditionFormula>AND 
(
ISPICKVAL(Service_Contract__r.Status__c, &quot;Awaiting Cust Signoff on Change Order&quot;), 
NOT(ISBLANK(M1_proof_panel_inverter_delivery__c)) 
)</errorConditionFormula>
        <errorMessage>M1 cannot be entered if there is an outstanding change order.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Outstanding_Change_order_M2</fullName>
        <active>true</active>
        <description>M2 cannot be entered if there is an outstanding change order.</description>
        <errorConditionFormula>AND 
( 
ISPICKVAL(Service_Contract__r.Status__c, &quot;Awaiting Cust Signoff on Change Order&quot;), 
NOT(ISBLANK(M2_proof_substantial_completion__c)) 
)</errorConditionFormula>
        <errorMessage>There is an outstanding change order for this customer.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PTO_cannot_have_date_in_future</fullName>
        <active>true</active>
        <description>PTO date cannot be in the future</description>
        <errorConditionFormula>PTO__c &gt; TODAY()</errorConditionFormula>
        <errorMessage>PTO date cannot be in the future</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Permit_Dates_Error</fullName>
        <active>false</active>
        <description>Permit Submitted Date can be specified only if Permit Expected Date is specified and vice versa.</description>
        <errorConditionFormula>OR( 
AND(NOT(ISNULL(Permit_Submitted_Date__c)) , ISNULL(Permit_Expected_Date__c) , ISNULL(Permit_Approval_Date__c)), 
AND(NOT(ISNULL(Permit_Expected_Date__c) ) , ISNULL(Permit_Submitted_Date__c), ISNULL(Permit_Approval_Date__c)) 
)</errorConditionFormula>
        <errorMessage>Permit Submitted Date can be specified only if Permit Expected Date is specified and vice versa</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Rate_Modification_checkbox_and_date</fullName>
        <active>true</active>
        <description>Both Rate Modification and Rate Modification Date must be populated together.</description>
        <errorConditionFormula>OR( 
(Rate_Modification__c = FALSE &amp;&amp; NOT(ISNULL(Rate_Modification_Date__c))), 
