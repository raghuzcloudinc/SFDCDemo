    <decisions>
        <name>Is_User_Brea</name>
        <label>Is User Brea?</label>
        <locationX>442</locationX>
        <locationY>417</locationY>
        <defaultConnectorLabel>No User Is Not Brea</defaultConnectorLabel>
        <rules>
            <name>Yes_User_Is_Brea</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>UserProfileName</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>Brea</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_user_as_Customer_Care_Brea_role</targetReference>
            </connector>
            <label>Yes User Is Brea</label>
        </rules>
    </decisions>
