            <targetReference>Is_user_Operations_for_Group_Assignment</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>User Is Not Customer Care</defaultConnectorLabel>
        <rules>
            <name>Yes_User_is_Customer_Care</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>UserProfileName</leftValueReference>
                <operator>StartsWith</operator>
                <rightValue>
                    <stringValue>Customer Care</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_user_as_Knowledge_User_and_default_Customer_Care_role</targetReference>
            </connector>
            <label>Yes User is Customer Care</label>
        </rules>
    </decisions>
    <decisions>
