        <locationX>760</locationX>
        <locationY>159</locationY>
        <defaultConnector>
            <targetReference>Is_User_Profile_Five9</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>User Is Not Operations</defaultConnectorLabel>
        <rules>
            <name>Yes_User_is_Operations</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>ProfileID</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
