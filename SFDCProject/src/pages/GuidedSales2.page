<apex:page standardStylesheets="false" showHeader="false" sidebar="false">
    <apex:includeScript value="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.4/jquery.min.js"/>
    
  <style>
  html, body {
    height: 100%;
  }

  body {
    padding: 8px;
    background-color: #F6F6F6;
    box-sizing: border-box;
  }

  .split {
    -webkit-box-sizing: border-box;
       -moz-box-sizing: border-box;
            box-sizing: border-box;

    overflow-y: auto;
    overflow-x: hidden;
  }

  .content {
    border: 1px solid #C0C0C0;
    box-shadow: inset 0 1px 2px #e4e4e4;
    background-color: #fff;
  }

  .gutter {
    background-color: transparent;
    background-repeat: no-repeat;
    background-position: 50%;
  }

  .gutter.gutter-horizontal {
    cursor: col-resize;
    background-image: url('https://nathancahill.github.io/Split.js/grips/vertical.png');
  }

  .gutter.gutter-vertical {
    cursor: row-resize;
    background-image: url('https://nathancahill.github.io/Split.js/grips/horizontal.png');
  }

  </style>

    <div id="sobj-details" class="split split-vertical">
        <div class="split content" style="width: 100%; height: 100%">
            Test
        </div>
    </div>
    <div id="dynamic-details" class="split split-vertical">
      <div id="guided-sales" class="split split-horizontal" style="width: 100%; height: 100%;">
          <div class="split content">
              content3
          </div>
      </div>
      <div id="dynamic-content" class="split split-horizontal content" style="width: 100%; height: 100%">
          <div class="split content">
              content4
          </div>
      </div>
    </div>
    <apex:includeScript value="https://nathancahill.github.io/Split.js/split.js"/>
<script>
    Split(['#sobj-details', '#dynamic-details'], {
      gutterSize: 6,
      cursor: 'col-resize',
      direction: 'vertical'
    })

    Split(['#guided-sales', '#dynamic-content'], {
      gutterSize: 6,
      cursor: 'row-resize'
    })
</script>
</apex:page>