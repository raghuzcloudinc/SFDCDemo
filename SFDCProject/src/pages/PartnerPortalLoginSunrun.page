    <section class="container content-container">
        <div class="bg-container">
            <div class="address-picker ng-hide" ng-show="hasMultipleHomes">
                <select ng-change="getAddressProduction(address)" ng-model="address" ng-options="loc.address for loc in locations track by loc.id" class="ng-pristine ng-untouched ng-valid">
                    <option value="?" selected="selected"></option>
                </select>
            </div>
            <div class="row content inner">
                <!-- uiView:  -->
                <div ui-view="" autoscroll="true" class="main-view animated fadeIn ng-scope">
                    <div class="logo ng-scope">
                        <img src="{!URLFOR($Resource.CommunityResourcesAee, 'partner/images/sunrun_white_logo.png')}"/>
                    </div>
                    <div class="login col-sm-12 ng-scope">
                        <apex:form >
                        <div class="form">
                            <div class="form-group first-row row desktop-half">
                                <label for="email">User name</label>
                                <apex:inputtext size="30" id="username" styleClass="desktop-switch ng-pristine ng-untouched ng-valid-email ng-invalid ng-invalid-required" value="{!username}"/>
                            </div>
                            <div class="form-group row desktop-half">
                                <label for="password">Password</label>
                                <apex:inputSecret size="30" id="password" styleClass="desktop-switch ng-pristine ng-untouched ng-invalid ng-invalid-required"  value="{!password}"/>
                            </div>

                            <div class="form-group error" style="display:initial">
                                <c:PartnerPortalErrorMessage />
                            </div>
                            <div class="forgot-pass-btn clearfix">
                                <a ui-sref="password-a" class="white-link-uppercase" ng-click="appAnalytics(&#39;Login&#39;,&#39;Password&#39;,&#39;Forgot Password&#39;);" analytics-on="click" analytics-category="Login" analytics-event="Password" analytics-label="Forgot Password" href="/partner/PartnerPortalForgotPassword">or Password?</a>
                                <a ui-sref="password-a" class="white-link-uppercase" ng-click="appAnalytics(&#39;Login&#39;,&#39;Password&#39;,&#39;Forgot Password&#39;);" analytics-on="click" analytics-category="Login" analytics-event="Password" analytics-label="Forgot Password" href="/partner/PartnerPortalForgotUserName">Forgot Username?</a>
                            </div>
                            <div class="form-group sign-in-btn submit-btn-center">
                                <apex:commandButton styleClass="btn btn-gradient sfdc-submit-button" action="{!login}" value="    Sign In    " id="theButton1"/>
                            </div>
                        </div>
                        </apex:form>
                    </div>
                </div>
            </div>
        </div>
        <div class="registration ng-scope" ng-controller="authController">
            <div class="form-container clearfix" ng-hide="hideRegisterForm">
                <h2 class="reg-header" ng-hide="registerMessage">Get Started</h2>
                <p class="reg-copy" ng-hide="registerMessage">Get to the good stuff! Please give us your email address to find your account</p>
                <form name="registerForm" ng-submit="register(register.email)" ng-hide="registerMessage" class="ng-pristine ng-valid-email ng-invalid ng-invalid-required">
                    <input type="email" placeholder="Email Address" ng-model="register.email" text-box="" required="" class="ng-pristine ng-untouched ng-valid-email ng-invalid ng-invalid-required" autocomplete="off" style="background-image: url(&quot;data:image/png;base64,&quot;); background-attachment: scroll; background-position: 100% 50%; background-repeat: no-repeat;"/>
                    <div class="reg-btns">
                        <input type="submit" class="btn btn-gradient" value="Find"/>
                        <a href="" ng-click="getStarted()" class="btn btn-gradient btn-right">Cancel</a>
                    </div>
                </form>
                <div class="message">
                    <p ng-bind-html="registerMessage | to_trusted" class="ng-binding"></p>
                </div>
            </div>
        </div>
        <div class="push"></div>
    </section>
